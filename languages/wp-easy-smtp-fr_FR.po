msgstr "Intervenants"

#: wp-easy-smtp.php:331
msgid "Custom"
msgstr "Custom"

#: wp-easy-smtp.php:335
msgid "Enter the recipient's email address"
msgstr "Entrez l’adresse e-mail du destinataire"

#: wp-easy-smtp.php:340
msgid "Subject"
msgstr "Objet"

#: wp-easy-smtp.php:343
msgid "Enter a subject for your message"
msgstr "Entrez l’objet de votre message"

#: wp-easy-smtp.php:343 wp-easy-smtp.php:350
msgid "Variable values are:"
msgstr "Variable valeurs sont:"

#: wp-easy-smtp.php:347
msgid "Message"
msgstr "Message"

#: wp-easy-smtp.php:350
